class Solution():
    def generate(self,str,ans,open,close,n):
        if open+close==2*n and open==close:
            ans.append(str)
        if open>n:
            return
        if close>open:
            return
        self.generate(str+"(",ans,open+1,close,n)
        self.generate(str+")",ans,open,close+1,n)
    def paranthesis(self,n):
        str=""
        ans=[]
        open=0
        close=0
        self.generate(str,ans,open,close,n)
        return ans
ob=Solution()
n=3
print(ob.paranthesis(n))
